{
  "name": "worklist",
  "version": "1.0.0",
  "description": "A todo list app",
  "main": "app.js",
  "scripts": {
    "local": "nodemon server/app --exec babel-node",
    "test": "mocha --require babel-register server/tests/**/*.js --timeout 100000 --exit",
    "coverage": "nyc --reporter=html --reporter=text --reporter=lcov mocha --reporter spec --require babel-register server/tests/**/*.js --timeout 600000 --exit",
    "coveralls": "npm run coverage -- --report lcovonly && cat coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tundewritescode/WorkList.git"
  },
  "author": "Babatunde Adeyemi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tundewritescode/WorkList/issues"
  },
  "homepage": "https://github.com/tundewritescode/WorkList#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "cloudinary": "^1.9.1",
    "del": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.9.0",
    "express": "^4.16.2",
    "express-fileupload": "^0.3.0",
    "express-validator": "^4.2.1",
    "jsonwebtoken": "^8.1.0",
    "mongoose": "^4.12.3",
    "save-dev": "^2.0.0",
    "supertest": "^3.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1"
  }
}
